public class String {

  char[] value;
  int count;
  int offset;
  
  static char emptyStringValue[] = new char[0];
  
  public String()
  {
    value = emptyStringValue;
    offset = 0;
    count = 0;
  }
  
  public String(String str)
  {
    value = str.value;
    offset = str.offset;
    count = str.count;
  }

}
